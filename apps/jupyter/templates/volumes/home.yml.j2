apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: jupyter-pvc-home
  namespace: "{{ namespace_name }}"
  labels:
    app: jupyter
    service: app
spec:
  accessModes:
  - ReadWriteMany
  resources:
    requests:
      storage: {{ jupyter_home_volume_size }}
  storageClassName: "{{ default_storage_class_rwx }}"
